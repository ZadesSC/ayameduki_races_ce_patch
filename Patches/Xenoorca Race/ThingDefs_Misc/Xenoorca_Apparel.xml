<?xml version='1.0' encoding='utf-8'?>
<Patch>
  <Operation Class="PatchOperationSequence">
	<operations>
	  <li Class="PatchOperationFindMod">
			
		<mods><li>[Aya]Xenoorca Race</li></mods>
			
		<match Class="PatchOperationSequence">
		<operations>

        <!--========= Abysswear =========-->
        <li Class="PatchOperationReplace">
				<xpath>Defs/ThingDef[defName="HAR_XO_AT_x"]/statBases/StuffEffectMultiplierArmor</xpath>
				<value>
				<StuffEffectMultiplierArmor>3</StuffEffectMultiplierArmor>
				<Bulk>3</Bulk>
				<WornBulk>1</WornBulk>
				</value>
		</li>

        <!--========= Child's xenowear =========-->
        <li Class="PatchOperationReplace">
				<xpath>Defs/ThingDef[defName="Aya_Race_Ap_XO_C_T_a"]/statBases/StuffEffectMultiplierArmor</xpath>
				<value>
				<StuffEffectMultiplierArmor>2</StuffEffectMultiplierArmor>
				<Bulk>2</Bulk>
				<WornBulk>0</WornBulk>
				</value>
		</li>

        <!--========= Imperial Armor =========-->
        <li Class="PatchOperationReplace">
				<xpath>Defs/ThingDef[defName="HAR_XO_AM_c"]/statBases/ArmorRating_Sharp</xpath>
				<value>
				<ArmorRating_Sharp>24</ArmorRating_Sharp>
				<Bulk>80</Bulk>
				<WornBulk>10</WornBulk>
				</value>
		</li>

        <li Class="PatchOperationReplace">
				<xpath>Defs/ThingDef[defName="HAR_XO_AM_c"]/statBases/ArmorRating_Blunt</xpath>
				<value>
				<ArmorRating_Blunt>52</ArmorRating_Blunt>
				</value>
		</li>

        <!--========= Imperial Armor =========-->
        <li Class="PatchOperationReplace">
				<xpath>Defs/ThingDef[defName="HAR_XO_AH_h"]/statBases/ArmorRating_Sharp</xpath>
				<value>
				<ArmorRating_Sharp>22</ArmorRating_Sharp>
				<Bulk>40</Bulk>
				<WornBulk>10</WornBulk>
				</value>
		</li>

        <li Class="PatchOperationReplace">
				<xpath>Defs/ThingDef[defName="HAR_XO_AH_h"]/statBases/ArmorRating_Blunt</xpath>
				<value>
				<ArmorRating_Blunt>48</ArmorRating_Blunt>
				</value>
		</li>

        </operations>
		</match>	
	  </li>
	</operations>	
  </Operation>
</Patch>